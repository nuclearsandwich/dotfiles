super + {_,shift} {1-9,0}
  bspc {desktop -f,node -d} {term, www, chat, dev, mail, bonus, 5000, extra, nine, root}

super + m
  bspc desktop -l monocle

super + t
  bspc desktop -l tiled

super {_,shift} + {h,j,k,l}
  bspc node -{f,s} {left, down, up, right}

super + {comma,period}
	bspc node -f {prev,next}

super + Tab
  bspc desktop -f last

super + b
	bspc node -s biggest

super + shift + c
	bspc node --close
